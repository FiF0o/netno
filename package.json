{
  "name": "react-webpack-starter",
  "version": "0.3.1",
  "description": "React webpack starter template",
  "main": "lib/index.js",
  "dependencies": {
    "eslint-config-airbnb-base": "^5.0.2",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-router": "^2.6.1"
  },
  "author": {
    "name": "Jon Lazarini",
    "email": "",
    "url": ""
  },
  "license": "MIT",
  "keywords": [
    "react",
    "carousel",
    "responsive"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:krasimir/react-webpack-starter.git"
  },
  "scripts": {
    "validate-webpack:dev": "webpack-validator webpack.config.js --env.dev",
    "validate-webpack:prod": "webpack-validator webpack.config.js --env.prod",
    "dev": "./node_modules/.bin/webpack --watch --inline",
    "build": "./node_modules/.bin/webpack --config=webpack.build.config.js",
    "test": "karma start",
    "test:ci": "watch 'npm run test' src/",
    "test:coverage": "istanbul cover _mocha -- --recursive --require babel-core/register src/**/*.test.*",
    "watch:test": "npm test --auto-watch --no-single-run",
    "prestart": "npm run clean-and-copy",
    "start": "webpack-dev-server --config webpack.config.js",
    "sasslint": "sass-lint -config .sass-lint.yml src/sass/*.sass --verbose --no-exit --ignore src/vendors/**",
    "eslint": "eslint src/app/*.js",
    "prebuild": "npm run clean-and-copy",
    "prebuild:prod": "npm run clean-and-copy && mkdir-dist",
    "clean-and-copy": "npm run clean-dist && npm run create-dist && npm run copy-files",
    "clean-dist": "rimraf-dist",
    "create-dist": "mkdir-dist",
    "copy-files": "cp src/favicon.ico public",
    "postinstall": "npm rebuild node-sass"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.6.5",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "bootstrap-sass": "^3.3.7",
    "chai": "3.3.0",
    "codebird": "^2.6.0",
    "core-js": "1.1.4",
    "css-loader": "^0.23.1",
    "eslint": "^3.3.1",
    "eslint-loader": "1.5.0",
    "eslint-plugin-import": "^1.13.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.1.2",
    "estraverse-fb": "^1.3.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "firebase": "^3.2.1",
    "html-webpack-plugin": "^2.22.0",
    "jquery": "^3.1.0",
    "karma": "^0.13.19",
    "karma-chai": "0.1.0",
    "karma-chai-plugins": "0.6.0",
    "karma-chai-sinon": "0.1.5",
    "karma-chrome-launcher": "0.2.0",
    "karma-mocha": "0.2.0",
    "karma-mocha-reporter": "1.1.1",
    "karma-phantomjs-launcher": "0.2.1",
    "karma-sinon": "1.0.4",
    "karma-sinon-chai": "1.1.0",
    "karma-sourcemap-loader": "0.3.5",
    "karma-webpack": "1.7.0",
    "mocha": "^2.3.4",
    "phantomjs": "^1.9.19",
    "re-base": "^1.5.1",
    "react": "^15.2.0",
    "react-addons-test-utils": "15.2.0",
    "react-dom": "^15.2.0",
    "react-router": "^2.6.1",
    "sass-lint": "^1.8.2",
    "sass-loader": "^4.0.0",
    "sasslint-webpack-plugin": "^1.0.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "watch": "0.16.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.11.0",
    "webpack-validator": "^2.2.7"
  }
}
